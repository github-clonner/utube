  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env=dev --config=./config/webpack.config.js",
    "test": "npm run test:lint && npm run test:e2e && npm run test:unit && npm run test:coverage && npm run test:coverage:threshold",
    "test:lint": "eslint . --ext .js",
    "test:e2e": "nightwatch",
    "test:unit": "NODE_PATH=./src mocha --require babel-register ./src/utils/test-helper.js ./src/**/*.test.js --recursive",
    "test:watch": "npm run test:unit -- --watch",
    "test:coverage": "istanbul cover _mocha -- --require babel-register ./src/utils/test-helper.js --recursive ./src/**/*.test.js",
    "test:coverage:threshold": "node ./node_modules/.bin/istanbul check --branches 100 --functions 100 --statements 100 --lines 100  ./coverage/coverage.json",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build": "webpack --env=prod --config=./config/webpack.config.js --progress --profile --colors",
    "build:dev": "webpack --env=dev --config=./config/webpack.config.js --progress --profile --colors"
  },
